{
  "extends": "eslint:recommended",
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "rules": {
    "semi": [
      "error",
      "always"
    ],
    "quotes": [
      "error",
      "double"
    ]
  },
  "no-console": [
    "error",
    {
      "allow": [
        "warn",
        "debug",
        "error"
      ]
    }
  ]
}